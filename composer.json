{
    "name": "symplify/console-package-builder",
    "description": "Package to speed up building command line applications",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^5.3",
        "symplify/symplify-kernel": "^9.4.13"
    },
    "require-dev": {
        "symfony/http-kernel": "^5.3",
        "symplify/package-builder": "^9.4.13",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ConsolePackageBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ConsolePackageBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
